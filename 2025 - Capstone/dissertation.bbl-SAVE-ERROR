    \entry{huttenrauch2019}{online}{}{}
      \name{author}{3}{}{%
        {{hash=57c5488cca6508b0a6dc29ab3c9afe54}{%
           family={Hüttenrauch},
           familyi={H\bibinitperiod},
           given={Maximilian},
