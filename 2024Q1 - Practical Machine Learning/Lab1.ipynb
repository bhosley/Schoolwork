    "def fibonacci(len=10,i=0,j=1):\n",
    "    if(len==0):\n",
    "        return\n",
    "    else:\n",
    "        print(f\"\\n{i}\")\n",
    "        len=len-1\n",
    "        i,j = j,i+j\n",
    "        fibonacci(len,i,j)"
   ]
